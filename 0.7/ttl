bevon:Aging
    dcterms:issued "2013-07-04+09:00"^^xsd:date ;
    dcterms:modified "2014-03-01+09:00"^^xsd:date ;
    a owl:Class ;
    rdfs:isDefinedBy <http://rdfs.co/bevon/> ;
    status:term_status "unstable"@en ;
    skos:definition """
    Aging is the process of storing beverages in casks for a specific period of
    time to remove harsh flavors and add distinct characteristics found in the
    cask's wood.
    """@en ;
    skos:prefLabel "Aging"@en, "숙성"@ko .

bevon:AlcoholicBeverage
    dcterms:issued "2013-07-04+09:00"^^xsd:date ;
    dcterms:modified "2014-03-01+09:00"^^xsd:date ;
    a owl:Class ;
    rdfs:isDefinedBy <http://rdfs.co/bevon/> ;
    rdfs:seeAlso <http://en.wikipedia.org/wiki/Alcoholic_beverage> ;
    rdfs:subClassOf bevon:Beverage, pto:Alcoholic_beverage ;
    owl:disjointWith bevon:NonAlcoholicBeverage ;
    status:term_status "testing"@en ;
    skos:definition """
     An alcoholic beverage is a drink and psychoactive drug containing
     ethyl alcohol which is commonly referred to as ethanol.
     """@en ;
    skos:prefLabel "Alcoholic Beverage"@en, "알코올 음료"@ko .

